<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhxh.xanim">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.zhxh.xanim.AnimateLayoutChanges" >
        </activity>
        <activity android:name="com.zhxh.xanim.AnimateFreeFall" >
        </activity>
        <activity android:name="com.zhxh.xanim.AnimateMoveInSecond" >
        </activity>
        <activity android:name="com.zhxh.xanim.AnimateMultiple" >
        </activity>
        <activity android:name="com.zhxh.xanim.AnimateLayoutTransition" >
        </activity>
        <activity android:name="com.zhxh.xanim.AnimateLayoutAnimation" >
        </activity>
    </application>

</manifest>